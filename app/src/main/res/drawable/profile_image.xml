<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background rounded rectangle shape -->
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100dp" /> <!-- Adjusted radius for full rounding -->
            <solid android:color="#CFCCF6" />
        </shape>
    </item>
    <!-- Image -->
    <item
        android:drawable="@drawable/background"
        android:gravity="center"
        android:width="75dp"
        android:height="75dp">
    <shape android:shape="rectangle">
        <corners android:radius="50dp" /> <!-- Set the same radius as the background shape -->
    </shape>
</item>
    </layer-list>
